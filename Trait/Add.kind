// Represents the Add trait for a type A
Trait/Add
: ∀(A: *)
  *
= λA data[]{
  #MkAdd{
    add: ∀(x: A) ∀(y: A) A
  } : (Trait/Add A)
} : (Trait/Add A)

// Accessor for the add field
Trait/Add/add
: ∀(A: *)
  ∀(add: (Trait/Add A))
  ∀(x: A)
  ∀(y: A)
  A
= λA λ{
  #MkAdd: λadd.add λx λy (add.add x y)
}
