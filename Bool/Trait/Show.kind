use Bool/ as B/
use Trait/ as T/

// Instance of Show for Bool
B/Trait/Show/Bool : (T/Show B/Bool) = 
  #Show{
    to_string: B/show
  }


//TEST//

use Bool/ as B/
use Equal/ as E/

B/T0
: (E/Equal B/Bool (B/show #True{}) (#True{}))
= #Refl{}

B/T1
: (E/Equal B/Bool (B/show #False{}) #False{})
= #Refl{}

