// Bend/Fun/Pattern/mmap-binds

Bend/Fun/Pattern/mmap-binds <M: Type -> Type> (m: Monad M) (f: (Maybe String) -> M (Maybe String)) (pat: Bend/Fun/Pattern) : (M Bend/Fun/Pattern)
| m f (#Var x) =
  do m {
    ask x = (f x)
    return (#Var x)
  }
| m f pat =
  (Bend/Fun/Pattern/mmap-children m (Bend/Fun/Pattern/mmap-binds m f) pat)
