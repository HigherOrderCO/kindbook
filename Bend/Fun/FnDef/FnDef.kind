// Function definition
data Bend/Fun/FnDef {
  #FnDef {
    nam : String                // Name of the function
    typ : Bend/Fun/Type         // Type of the function
    chk : Bool                  // Whether to check the function
    rul : (List Bend/Fun/Rule)  // Pattern_matching rules defining the function
    src : Bend/Fun/Source           // Source location of the function
  }
}
