use Bend/Parser as BP

Bend/Parser/skip_newlines : (Parser U64)
| = do Parser {
  ask n = (BP/skip_trivia_inline)
  ask c = (Parser/peek_one)
  match c {
    #Some{c}:
      if (Char/eq c '\n') do Parser {
        ask (Parser/advance_one)
        (BP/skip_newlines)
      } else if (Char/eq c '\r') do Parser{
        ask (Parser/advance_one)
        (BP/skip_newlines)
      } else {
        (Parser/pure _ n)
      }
    #None: (Parser/pure _ n)
  }
}
