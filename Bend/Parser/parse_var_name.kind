// Parses a variable name.
// = Parser that returns the parsed variable name if successful.
Bend/Parser/parse_var_name : (Parser String)
= (Bend/Parser/parse_name "variable")

#test: (Parser/run String (Bend/Parser/parse_var_name) "validName") == #Some{"validName"}
#test: (Parser/run String (Bend/Parser/parse_var_name) "123invalid") == #None
#test: (Parser/run String (Bend/Parser/parse_var_name) "") == #None
#test: (Parser/run String (Bend/Parser/parse_var_name) "valid_name") == #Some{"valid_name"}
#test: (Parser/run String (Bend/Parser/parse_var_name) "Valid_Name_123") == #Some{"Valid_Name_123"}