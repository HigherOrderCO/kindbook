use BinMap/ as BM/

// Creates an empty BinMap.
// - A: The type of values that would be stored in the map.
// = An empty BinMap.
BM/empty
: ∀(A: *)
  (BM/BinMap A)
= λA (BM/Leaf A)
//TEST//

use BinMap/ as BM/
use Equal/ as E/
use Nat/ as N/

// Test case to verify that an empty BinMap is equivalent to a Leaf
T0
: (E/Equal (BM/BinMap N/Nat) (BM/empty N/Nat) (BM/Leaf N/Nat))
= #Refl{}