// Checks if a V2 vector is zero.
// - v: The V2 vector to check.
// = True if both components are zero, False otherwise.
V2/is-zero : V2 -> Bool
| #V2{x y} = (Bool/and (U32/is-zero x) (U32/is-zero y))

#test: (V2/is-zero #V2{x: 0 y: 0}) == #True
#test: (V2/is-zero #V2{x: 1 y: 0}) == #False
#test: (V2/is-zero #V2{x: 0 y: 1}) == #False
#test: (V2/is-zero #V2{x: 1 y: 1}) == #False
