// Checks if two strings are equal.
// - a: The first string to compare.
// - b: The second string to compare.
// = A boolean value indicating whether the strings are equal.
String/eq
: ∀(a: String)
  ∀(b: String)
  Bool
= λ{
  #Nil: λ{
    #Nil: #True
    #Cons: λb.head λb.tail #False
  }
  #Cons: λa.head λa.tail λ{
    #Nil: #False
    #Cons: λb.head λb.tail
      let head_eq = (U32/eq a.head b.head)
      let tail_eq = (String/eq a.tail b.tail)
      (Bool/and head_eq tail_eq)
  }
}
