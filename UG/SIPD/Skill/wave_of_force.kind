use UG/SIPD as G

// Creates a wave of force skill.
// - pid: Player ID.
// - duration: Effect duration.
// - wave_radius: Radius of the wave.
// - cooldown: Skill cooldown.
// = A wave of force skill.
G/Skill/wave_of_force : U64 -> U64 -> F64 -> U64 -> G/Skill
| pid duration wave_radius cooldown = 
  #Skill{
    name: "wave_of_force"
    effects: [(G/Effect/wave_of_force pid duration wave_radius)]
    cooldown
  }
