// Compares two F64 numbers.
// - a: First F64 number.
// - b: Second F64 number.
// = Cmp value: LT if a < b, EQ if a = b, GT if a > b.
F64/compare : F64 -> F64 -> Cmp
= λa λb
  if (U32/to-bool (F64/eql a b)) {
    #EQ
  } else {
    if (U32/to-bool (F64/ltn a b)) {
      #LT
    } else {
      #GT
    }
  }

#test: (F64/compare 1.0 1.0) == #EQ
#test: (F64/compare 1.0 2.0) == #LT
#test: (F64/compare 2.0 1.0) == #GT
