// Computes the length of a list while preserving the original list.
// - 1st: The type of elements in the list.
// - 2nd: The input list.
// = A pair containing the original list and its length as a Nat.
List/length-got
: ∀(A: *)
  ∀(xs: (List/List A))
  (Pair/Pair (List/List A) Nat/Nat)
= λA λ{
  #Nil: #Pair{[] #0}
  #Cons: λhead λtail
    let #Pair{rest n} = (List/length-got A tail)
		#Pair{(#Cons{head rest}) (#Succ{n})}
}
// Test: Empty list
TEST_0
: (Equal/Equal
    (Pair/Pair (List/List Nat/Nat) Nat/Nat)
    (List/length-got Nat/Nat [])
    #Pair{[] #0})
= #Refl

// Test: Single element list
TEST_1
: (Equal/Equal
    (Pair/Pair (List/List Nat/Nat) Nat/Nat)
    (List/length-got Nat/Nat (#Cons{#0 []}))
    #Pair{(#Cons{#0 []}) #1})
= #Refl

// Test: Multiple element list
TEST_2
: (Equal/Equal
    (Pair/Pair (List/List Nat/Nat) Nat/Nat)
    (List/length-got Nat/Nat (#Cons{#0 (#Cons{#1 (#Cons{#2 []})})}))
    #Pair{(#Cons{#0 (#Cons{#1 (#Cons{#2 []})})}) #3})
= #Refl