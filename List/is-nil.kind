// Checks if a list is empty.
// - A: Element type.
// - xs: Input list.
// = True if empty, False otherwise.
List/is-nil : âˆ€(A: *) (List A) -> Bool
| A #Nil             = #True
| A #Cons{head tail} = #False

#test: (List/is-nil U32 []) == #True
#test: (List/is-nil U32 [1]) == #False
#test: (List/is-nil U32 [1 2 3]) == #False
