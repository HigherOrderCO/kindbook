// Checks if two lists are equal.
List/eq : âˆ€(A: *) (IsEqual A) -> (List A) -> (List A) -> Bool
| A e #Nil        #Nil        = #True
| A e #Nil        #Cons{y ys} = #False
| A e #Cons{x xs} #Nil        = #False
| A e #Cons{x xs} #Cons{y ys} = 
  if (IsEqual/eq A e x y) {
    (List/eq A e xs ys)
  } else {
    #False
  }
