// Proof that is-nil returns True for an empty list
List/is-nil-empty
: ∀(A: *)
  (Equal/Equal Bool/Bool (List/is-nil A #Nil{}) #True{})
= λA #Refl{}

// Proof that is-nil returns False for a non-empty list
List/is-nil-cons
: ∀(A: *)
  ∀(x: A)
  ∀(xs: (List/List A))
  (Equal/Equal Bool/Bool (List/is-nil A (#Cons{x xs})) #False{})
= λA λx λxs #Refl{}

