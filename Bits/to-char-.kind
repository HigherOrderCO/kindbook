use Bits/ as BS/
use Char/ as C/
use Equal/ as E/
use U32/ as U32/

// Test: Convert 'A' (Unicode 65) to Char
T0
: (E/Equal C/Char (BS/to-char #I{#O{#O{#O{#O{#O{#I{#E}}}}}}}) 'A')
= #Refl

