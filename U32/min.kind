// Returns the minimum of two U32 numbers.
// - a: The first U32 number.
// - b: The second U32 number.
// = The smaller of the two input numbers.
U32/min : U32 -> U32 -> U32
= λa λb
  if (U32/to-bool (<= a b)) {
    a
  } else {
    b
  }

#test: (U32/min 5 3) == 3
#test: (U32/min 3 5) == 3
#test: (U32/min 5 5) == 5
