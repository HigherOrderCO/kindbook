// Compares two U32 numbers.
// - a: The first U32 number to compare.
// - b: The second U32 number to compare.
// = An Ord value: LT if a < b, EQ if a = b, GT if a > b.
U32/compare : U32 -> U32 -> Ord
= λa λb
  if (U32/to-bool (U32/eql a b)) {
    #EQ
  } else {
    if (U32/to-bool (< a b)) {
      #LT
    } else {
      #GT
    }
  }

#test: (U32/compare 5 3) == #GT
#test: (U32/compare 3 5) == #LT
#test: (U32/compare 5 5) == #EQ
