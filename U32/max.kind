// Returns the maximum of two U32 numbers.
// - a: The first U32 number.
// - b: The second U32 number.
// = The larger of the two input numbers.
U32/max : U32 -> U32 -> U32
= λa λb
  if (U32/to-bool (>= a b)) {
    a
  } else {
    b
  }

#test: (U32/max 5 3) == 5
#test: (U32/max 3 5) == 5
#test: (U32/max 5 5) == 5
